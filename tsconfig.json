{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"rootDir": ".",
		"baseUrl": "./src",
		"outDir": "./dist",
		"paths": {
			"@modules/*": [
				"modules/*"
			],
			"@config/*": [
				"config/*"
			],
			"@errors/*": [
				"errors/*"
			],
			"@utils/*": [
				"utils/*"
			],
			"@middlewares/*": [
				"middlewares/*"
			],
			"@prisma/*": [
				"prisma/*"
			],
			"@routes/*": [
				"routes/*"
			]
		},
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"typeRoots": [
			"./src/@types"
		],
		"types": ["@types/jest"]
	}
}