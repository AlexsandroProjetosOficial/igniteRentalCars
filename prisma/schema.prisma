generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id          String   @id @default(uuid())
  name        String
  description String
  createdAt   DateTime @default(now())

  @@map("categories")
}

model Specification {
  id          String   @id @default(uuid())
  name        String
  description String
  createdAt   DateTime @default(now())

  @@map("specifications")
}
